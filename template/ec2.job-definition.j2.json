{
  "jobDefinitionName": "{{ job_definition_name }}",
  "type": "container",
  "containerProperties": {
    "image": "{{ image }}",
    "vcpus": 1,
    "memory": 1024,
    "command": [
      "--version"
    ],
    "jobRoleArn": "{{ job_role_arn }}",
    "executionRoleArn": "{{ execution_role_arn }}",
    "environment": [
      {
        "name": "INPUT_DATA_DIR",
        "value": "/input"
      },
      {
        "name": "OUTPUT_DATA_DIR",
        "value": "/output"
      }
    ]
  },
  "retryStrategy": {
    "attempts": 2,
    "evaluateOnExit": [
      {
        "onStatusReason": "",
        "onReason": "",
        "onExitCode": "",
        "action": "EXIT"
      }
    ]
  },
  "timeout": {
    "attemptDurationSeconds": 86400
  },
  "platformCapabilities": [
    "EC2"
  ]
}
