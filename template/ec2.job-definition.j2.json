{
  "jobDefinitionName": "{{ job_definition_name }}",
  "type": "container",
  "parameters": {
    "inputDataDir": "/input",
    "outputDataDir": "/output"
  },
  "containerProperties": {
    "image": "{{ image }}",
    "vcpus": 0,
    "memory": 0,
    "command": [
      "--version"
    ],
    "jobRoleArn": "{{ job_role_arn }}",
    "executionRoleArn": "{{ execution_role_arn }}",
    "environment": [
      {
        "name": "INPUT_DATA_DIR",
        "value": "Ref::inputDataDir"
      },
      {
        "name": "OUTPUT_DATA_DIR",
        "value": "Ref::outputDataDir"
      }
    ],
    "readonlyRootFilesystem": false,
    "privileged": false,
    "user": "root"
  },
  "retryStrategy": {
    "attempts": 5
  },
  "timeout": {
    "attemptDurationSeconds": 604800
  },
  "platformCapabilities": [
    "EC2"
  ]
}
